datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// prisma/schema.prisma


model Product {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  price       String
  photos      Photo[]
}
model Photo {
  id       Int    @id @default(autoincrement())
  url      String
  product  Product @relation(fields: [productId], references: [id])
  productId Int
}